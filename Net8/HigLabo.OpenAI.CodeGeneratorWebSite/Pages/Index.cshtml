</span></span><span><span>    </span><span class="hljs-attr">"created"</span><span>: </span><span class="hljs-number">1677858242</span><span>,
</span></span><span><span>    </span><span class="hljs-attr">"model"</span><span>: </span><span class="hljs-string">"gpt-4o-mini"</span><span>,
</span></span><span><span>    </span><span class="hljs-attr">"usage"</span><span>: {
</span></span><span><span>        </span><span class="hljs-attr">"prompt_tokens"</span><span>: </span><span class="hljs-number">13</span><span>,
</span></span><span><span>        </span><span class="hljs-attr">"completion_tokens"</span><span>: </span><span class="hljs-number">7</span><span>,
